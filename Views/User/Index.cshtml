@model IEnumerable<Car_Hire_Services__CHS_.ViewModel.CarViewModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_CustomerLayout.cshtml";
}

<div class="container">


    <table id="example" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Name</th>
                <th>Amount</th>
                <th>Brand</th>

            </tr>
        </thead>
        @if (Model != null && Model.Count() > 0)
        {
            <tbody>
                @foreach (var car in Model)
                {
                    <tr>
                        <td>@car.Name</td>
                        <td>@car.Amount</td>
                        <td>@car.Brand</td>
                    </tr>
                }


            </tbody>
        }

    </table>


</div>






